<div class="app-header">
  <nav class="navbar navbar-default custom-header">
    <div class="container-fluid">
      <div class="navbar-header">
        <a href="/">
          <%= image_tag "users/title_icon_back_normal.png" %>
        </a>
        <a href="#" >症状查询</a>
      </div>
    </div>
  </nav>
</div>

<div class="symptom-list-content">
  <% @symptom_first_chars.each do |char| %>
    <div class="letter"><%= char %></div>
    <ul class="symptom-list list-unstyled">
      <% $redis_symptoms.lrange(char, 0, -1).each do |s| %>
      <li class="symptom-item">
        <a href="symptoms/<%= s.split(',')[0] %>"> <%= s.split(',')[1] %></a>
      </li>
      <!-- <button class="more">加载更多</button> -->
      <% end %>
     </ul>
  <% end %>
</div>